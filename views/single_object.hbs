{{> navbar}}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{object.title}}</h1>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-8">
            <div class="table-responsive">
                <table class="table">
                    <tbody>
                    <tr>
                        <td><strong>Type</strong></td>
                        <td>{{object.type}}</td>
                    </tr>
                    <tr>
                        <td><strong>Dimensions</strong></td>
                        <td>{{object.dimensions}}</td>
                    </tr>
                    <tr>
                        <td><strong>Dated</strong></td>
                        <td>{{object.dated}}</td>
                    </tr>
                    <tr>
                        <td><strong>Artist</strong></td>
                        <td>{{object.artist}}</td>
                    </tr>
                    <tr>
                        <td><strong>ID</strong></td>
                        <td>{{object.id}}</td>
                    </tr>
                    <tr>
                        <td><strong>Active</strong></td>
                        <td>{{object.active}}</td>
                    </tr>


                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-lg-4">
            <img src="../img/oil.jpg" style="width:75%;height:75%">
        </div>
    </div>

    <br><br>

    <div class="panel panel-default">
        <div class="panel-heading">Extra Content</div>
        <div class="panel-body">
            <ul class="nav nav-tabs">
                <li class="active"><a href="#audio" data-toggle="tab">Audio</a>
                </li>
                <li><a href="#video" data-toggle="tab">Video</a>
                </li>
                <li><a href="#images" data-toggle="tab">Images</a>
                </li>
                <li><a href="#text" data-toggle="tab">Text</a>
                </li>
            </ul>

            <div class="tab-content">
                <div class="tab-pane fade in active" id="audio">
                    <h4>Audio Tab</h4>
                    <p>Audio goes here</p>
                </div>
                <div class="tab-pane fade" id="video">
                    <h4>Video Tab</h4>
                    <p>Video goes here</p>
                </div>
                <div class="tab-pane fade" id="images">
                    <h4>Images Tab</h4>
                    <p>Images goes here</p>
                </div>
                <div class="tab-pane fade" id="text">
                    <h4>Text Tab</h4>
                    <p>Text goes here</p>
                </div>
            </div>

        </div>

    </div>









</div>