{{> navbar}}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{title}}</h1>
        </div>
    </div>

    <div class="form-group input-group">
        <input type="text" class="form-control" placeholder="Search">
            <span class="input-group-btn">
                <button class="btn btn-default" type="button"><i class="fa fa-search"></i>
                </button>
        </span>
    </div>

    <div class="table-responsive">
        <table class="table">
            <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Gender</th>
                <th>Age</th>
                <th>Banned</th>
            </tr>
            </thead>
            <tbody id="table_body">
            {{#each users}}
                <tr>
                    <td>{{id}}</td>
                    <td>{{email}}</td>
                    <td>{{first_name}}</td>
                    <td>{{last_name}}</td>
                    <td>{{gender}}</td>
                    <td>{{age}}</td>
                    <td>{{banned}}</td>


                </tr>
            {{/each}}

            </tbody>
        </table>
    </div>







</div>