{{> navbar}}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{title}}</h1>
        </div>
    </div>




    <div class="dataTable_wrapper">
        <table class="table table-striped table-bordered table-hover" id="object_table">

        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Type</th>
                <th>Dimensions</th>
                <th>Date Added</th>
                <th>Artist</th>
                <th>View</th>
            </tr>
            </thead>
            <tbody id="table_body">
            {{#each objects}}
            <tr>
                <td>{{id}}</td>

                <td>{{title}}</td>

                <td>{{type}}</td>
                <td>{{dimensions}}</td>
                <td>{{dated}}</td>
                <td>{{Artist.name}}</td>
                <td style="text-align: center"><a href="/artifact/{{id}}"><button class="btn btn-primary">View</button></a></td>

            </tr>

            {{/each}}

            </tbody>
        </table>
    </div>




</div>