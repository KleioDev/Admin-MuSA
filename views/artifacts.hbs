{{> navbar}}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{title}}</h1>
        </div>
    </div>




    <div class="dataTable_wrapper">
        <table class="table table-striped table-bordered table-hover" id="object_table">

        <thead>
            <tr>
                <th>Title</th>
                <th>Type</th>
                <th>Dimensions</th>
                <th>Date Added</th>
                <th>Artist</th>
                <th>ID</th>
                <th>Active</th>
                <th>View</th>
            </tr>
            </thead>
            <tbody id="table_body">
            {{#each objects}}
            <tr>
                <td>{{title}}</td>

                <td>{{type}}</td>
                <td>{{dimensions}}</td>
                <td>{{dated}}</td>
                <td>{{artist}}</td>
                <td>{{id}}</td>

                {{#if active}}
                <td>Active</td>
                {{else}}
                <td>Inactive</td>
                {{/if}}
                <td style="text-align: center"><a href="/artifact/{{id}}">View</a></td>

            </tr>

            {{/each}}

            </tbody>
        </table>
    </div>




</div>