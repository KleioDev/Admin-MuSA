{{> navbar}}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{title}}</h1>
        </div>
    </div>

    <div class="form-group input-group">
        <input type="text" class="form-control" placeholder="Search">
            <span class="input-group-btn">
                <button class="btn btn-default" type="button"><i class="fa fa-search"></i>
            </button>
        </span>
    </div>

    <a href="/group_management">Group Management</a>

    <div class="table-responsive">
        <table class="table">
            <thead>
            <tr>
                <th>Title</th>
                <th>Type</th>
                <th>Dimensions</th>
                <th>Date Added</th>
                <th>Artist</th>
                <th>ID</th>
                <th>Active</th>
                <th>View</th>
            </tr>
            </thead>
            <tbody id="table_body">
            {{#each objects}}
            <tr>
                <td>{{title}}</td>

                <td>{{type}}</td>
                <td>{{dimensions}}</td>
                <td>{{dated}}</td>
                <td>{{artist}}</td>
                <td>{{id}}</td>

                {{#if active}}
                <td>Active</td>
                {{else}}
                <td>Inactive</td>
                {{/if}}
                <td style="text-align: center"><a href="single_object/{{id}}"><button type="button" class="btn btn-default">View</button></a></td>

            </tr>

            {{/each}}

            </tbody>
        </table>
    </div>




</div>