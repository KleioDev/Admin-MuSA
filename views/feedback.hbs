{{> navbar}}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{title}}</h1>
        </div>
    </div>
        <div class="dataTable_wrapper">
            <table class="table table-striped table-bordered table-hover" id="feedback_table">
                <thead>
                <tr>
                    <th>Feedback</th>
                    <th>Type</th>
                    <th>Date</th>
                    <th>Solved</th>
                    <th>Change Status</th>
                </tr>
                </thead>
                <tbody>
                {{#each feedback}}
                <tr class="odd gradeX">
                    <td>{{text}}</td>
                    <td>{{type}}</td>
                    <td>{{date}}</td>
                    {{#if solved}}
                        <td style="text-align: center">Solved</td>
                        <td style="text-align: center"><a href="#"><i class="fa fa-trash fa-fw"></i></a></td>

                    {{else}}
                        <td style="text-align: center">Pending</td>
                        <td style="text-align: center"><a href="#"><i class="fa fa-check fa-fw"></i></a></td>

                    {{/if}}

                </tr>
                {{/each}}

                </tbody>
            </table>
        </div>
    </div>







</div>